#include<iostream>
#include<math.h>
#include<algorithm>
using namespace std;

int fibo(int n)
{
    int a=0;
    int b=1;
    cout<<a<<",";
    cout<<b<<",";
    for(int i = 0;i<n;i++)
    {
        int c;
        c = a + b; 
        cout<<c<<",";
        a=b;
        b=c;
    }
}
int main()
{
    int n;
    cin>>n;
    fibo(n);
}
